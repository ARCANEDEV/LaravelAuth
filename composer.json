{
    "name":        "arcanedev/laravel-auth",
    "description": "Laravel Authorization package.",
    "homepage":    "https://github.com/ARCANEDEV/LaravelAuth",
    "keywords":    [
        "arcanedev", "laravel", "auth", "acl", "authorisation", "authorization", "users", "roles", "permissions"
    ],
    "authors":  [
        {
            "name":     "ARCANEDEV",
            "email":    "arcanedev.maroc@gmail.com",
            "homepage": "https://github.com/arcanedev-maroc",
            "role":     "Developer"
        }
    ],
    "type":    "library",
    "license": "MIT",
    "require": {
        "php":                  ">=5.6",
        "arcanedev/support":    "~3.20",
        "arcanesoft/contracts": "~1.5"
    },
    "require-dev": {
        "phpunit/phpcov":  "~3.0",
        "phpunit/phpunit": "~5.0"
    },
    "autoload": {
        "psr-4": {
            "Arcanedev\\LaravelAuth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Arcanedev\\LaravelAuth\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "testbench": "composer require --dev \"orchestra/testbench-browser-kit=~3.0\" \"orchestra/database=~3.0\""
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable" : true
}
