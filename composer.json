{
    "name":        "arcanedev/laravel-auth",
    "description": "Laravel Authorization package.",
    "homepage":    "https://github.com/ARCANEDEV/LaravelAuth",
    "keywords":    [
        "arcanedev", "laravel", "auth", "acl", "authorisation", "authorization", "users", "roles", "permissions"
    ],
    "authors":  [
        {
            "name":     "ARCANEDEV",
            "email":    "arcanedev.maroc@gmail.com",
            "homepage": "https://github.com/ARCANEDEV",
            "role":     "Developer"
        }
    ],
    "type":    "library",
    "license": "MIT",
    "require": {
        "php":                  ">=5.5.9",
        "arcanedev/support":    "~3.0",
        "arcanesoft/contracts": "~1.0"
    },
    "require-dev": {
        "orchestra/testbench":  "~3.1",
        "phpunit/phpcov":       "~2.0",
        "phpunit/phpunit":      "~4.0|~5.0"
    },
    "autoload": {
        "psr-4": {
            "Arcanedev\\LaravelAuth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Arcanedev\\LaravelAuth\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
